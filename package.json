{
    "name": "dank-movies",
    "private": true,
    "version": "0.1.0",
    "scripts": {
        "dev": "concurrently \"tailwindcss -i ./pb_hooks/pages/app.tailwind.css -o ./pb_hooks/pages/app.css --watch\" \"pocketbase serve --dir=pb_data --dev\"",
        "wsl": "wsl npm run dev",
        "start": "pocketbase serve --dir=pb_data --dev",
        "css": "tailwindcss -i ./pb_hooks/pages/app.tailwind.css -o ./pb_hooks/pages/app.css --watch\"",
        "push": "npx phio deploy dank-movies",
        "login": "npx phio login",
        "update": "pocketbase update",
        "typegen": "node scripts/typegen.mjs"
    },
    "description": "Dank movies watchlist application.",
    "author": {
        "name": "Justin Kim",
        "github": "kimjust6"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/kimjust6/pocket-movies"
    },
    "keywords": [
        "pocketpages",
        "pocketbase",
        "movies",
        "watchlist",
        "tmdb",
        "daisyui",
        "tailwindcss"
    ],
    "devDependencies": {
        "@tailwindcss/typography": "^0.5.16",
        "concurrently": "^9.2.1",
        "daisyui": "^4.12.23",
        "dotenv": "^17.2.3",
        "prettier": "^3.6.2",
        "prettier-plugin-ejs": "^1.0.3",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "tailwindcss": "^3.4.17"
    },
    "dependencies": {
        "pocketbase": "^0.26.5",
        "pocketpages": "^0.22.3",
        "pocketpages-plugin-js-sdk": "^0.2.0"
    },
    "files": [
        "pb_hooks",
        "*.md"
    ],
    "pockethost": {
        "instanceName": "dank-movies"
    }
}